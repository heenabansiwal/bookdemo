<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping>
 

 
<class name="bookpojo.Rentcustomertablepojo" table="Rentcustomertable">
   		<id name="rentcustid" type="int" >
   		<column name="rentcustid"/>
		<generator class="identity"></generator>
   	</id>
   	
   	<property name="rentcost"  type="int">
   	<column name="rentcost" length="30" not-null="true"></column>
    </property>
    
    <property name="bookisbn"  type="string">
   	<column name="bookisbn" length="30" not-null="true"></column>
    </property>
    </class>
 
 </hibernate-mapping>